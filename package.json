    "pug": "^2.0.0-rc.2",
    "ramda": "^0.24.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.0",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3"
  },
