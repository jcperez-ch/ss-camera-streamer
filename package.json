{
  "name": "react-rollup-typescript-boilerplate",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "build": "npm-run-all -p build:*",
    "build:server": "rollup -c rollup.server.config.js",
    "build:client": "npm run lint && NODE_ENV=production rollup -c",
    "watch": "rollup -cw",
    "serve": "node server.js",
    "start": "yarn run build:server && npm-run-all -p watch serve",
    "lint": "tslint --project tsconfig.json --type-check"
  },
  "author": "",
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/express": "^4.0.36",
    "@types/ramda": "^0.24.0",
    "@types/react": "^15.0.16",
    "@types/react-dom": "^0.14.20",
    "@types/react-redux": "^4.4.44",
    "@types/socket.io": "^1.4.29",
    "@types/socket.io-client": "^1.4.29",
    "autoprefixer": "^7.1.1",
    "npm-run-all": "^4.0.2",
    "postcss": "^6.0.2",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^7.1.0",
    "rollup-plugin-livereload": "^0.4.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-sass": "^0.5.3",
    "rollup-plugin-serve": "^0.1.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^3.2.2",
    "tslib": "^1.6.0",
    "tslint": "^4.3.1",
    "typescript": "^2.2.0"
